TITLE     :={oDp:DPTIPDOCPRO}
TABLE     :=DPTIPDOCPRO
SELECT    :=TDC_TIPO,TDC_DESCRI,TDC_INVCON,TDC_INVFIS,TDC_INVLOG,TDC_IVA,TDC_LIBCOM,TDC_PAGOS,TDC_CXP,TDC_TRIBUT,TDC_GASCOM,TDC_ACTIVO,CIC_CUENTA,CTA_DESCRI,TDC_DOCDES,TDC_CLRGRA 
WHERE     :=
INNER     :=LEFT JOIN DPTIPDOCPRO_CTA ON CIC_CODIGO=TDC_TIPO LEFT JOIN DPCTA ON CIC_CTAMOD=CTA_CODMOD AND CIC_CUENTA=CTA_CODIGO
ORDER BY  :=TDC_TIPO  
GROUP BY  :=TDC_TIPO
WIDTH :=1045
HEIGHT:=920
APLICACION:=
TOPIC     :=
FILECHM   :=
LFULLHIGHT:=.T.    // Abarca todo el Alto Disponible
LHSCROLL  :=.T.
STYLE     :=0
COLORPANE2:=16771022
COLORPANE1:=16773345
FREEZE    :=0
FILTER_LOG :=DPTIPDOCPRO.TDC_ACTIVO
FILTER_TEXT:=Activos


