// Antes de Borrar
[BOTONES]
BTN01_BMP    :=xNEW.BMP
BTN01_ACTION :=SCRRUN("DPTIPDOCPRO",1,IIF(oCursor:Eof(),NIL,oCursor:TDC_TIPO))
BTN01_WHEN   :=(oDp:cIdApl="9")
